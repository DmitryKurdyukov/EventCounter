    ],
    "collectCoverageFrom": [
      "utils/**/*.{ts,tsx}",
      "zustand/**/*.{ts,tsx}",
      "components/**/*.{ts,tsx}",
      "app/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
